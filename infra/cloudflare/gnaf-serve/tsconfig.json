{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "declaration": false,
        "outDir": "./dist",
        "rootDir": "./src",
        "types": ["@cloudflare/workers-types"]
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules", "dist"]
}
